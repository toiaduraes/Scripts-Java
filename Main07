package main;

public class main07 {

	public static void main(String[] args) {
		// Média com notas pré-determinadas na string
		float nota1Bimestre = 10;
		float nota2Bimestre = 10;
		float nota3Bimestre = 5;
		float nota4Bimestre = 5;
		float media = (nota1Bimestre + nota2Bimestre + nota3Bimestre + nota4Bimestre) / 4;
		System.out.println("A média dos bimetres é: " + media);

	}

}
